  return (isIE) ? window[movieName] : document[movieName];
}
function Pause() 
{
	//alert(getFlashMovie("testata_flash").data);
	
    getFlashMovie("testata_flash").pause(); 
	$("#controller .play").show(); 
	$("#controller .pause").hide();
}
function Play() 
{
    getFlashMovie("testata_flash").play();   
	$("#controller .play").hide(); 
	$("#controller .pause").show();  
}
