var playing = false;
function callJavascriptStart()
{
	if(playing == false){
		playing = true;
		$("#shadow_roberto").fadeIn(2000);
		$("#frutta, #pub").animate({
			left: '+=229'
		  }, 2000);
		$("#fascia .first").animate({
			left: '+=20'
		  }, 2000);
		$("#fascia .second").animate({
			left: '+=10'
		  }, 2000);
	}
}
function callJavascriptEnd()
{
	if(playing == true){
		playing = false;
		$("#embedder").fadeOut("slow", function(){
			removeVideo();
		});
		$("#shadow_roberto").fadeOut("slow");
		$("#frutta, #pub").animate({
			left: '-=229'
		  }, 2000);
		$("#fascia .first").animate({
			left: '-=20'
		  }, 2000);
		$("#fascia .second").animate({
			left: '-=10'
		  }, 2000);
		$("#playVideo").fadeIn();
	}
}

$(document).ready(function(){
	if ($.cookie("vediVideo") != "1") {
		$("#playVideo").hide();
		$.cookie("vediVideo", "1", { expires: 7, path: '/' });
		insertVideo();
	}
	else{
		$("#embedder").hide();
	}
	
	$(window).resize(function(){
		var w = $(window).width();
		var newW = -1 * (w - 980) / 2;
		if(newW > 0) newW = 0;
		if(newW < -150) newW = -150;
		$("#playVideo").css("left", newW + "px");
	}).resize();
	
	if(navigator.appVersion.indexOf("MSIE 7") < 0 && navigator.appVersion.indexOf("MSIE 8") < 0){
		$("#embedder").hover(function(){
			$("#controller").show();
		},function(){
			if($("#controller .play").is(":visible") == false){
				$("#controller").hide();
			}
		});
	}
	
	$("#click").click(function(){
		$("#playVideo").fadeOut();
		$("#embedder").fadeIn();
		insertVideo();
	});
});

