		}

		if (error == false) {
			$("#form_contattaci_head").submit();
		}
	});
	
	function isEmail(string) {
		if (string.search(/^\w+((-\w+)|(\.\w+))*\@\w+((\.|-)\w+)*\.\w+$/) != -1)
			return true;
		else
			return false;
	}
});
